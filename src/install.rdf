<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>{0b62b504-857c-4f62-a336-2e8425bd5738}</em:id>     
    <em:version>0.9</em:version>
    
    <!-- Target Application this extension can install into, 
        with minimum and maximum supported versions. -->

    <!-- Thunderbird -->
    <em:targetApplication>
      <Description>
         <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
         <em:minVersion>3.0b1</em:minVersion>
         <em:maxVersion>10.*</em:maxVersion>
      </Description>
    </em:targetApplication>
   
    <!-- SeaMonkey -->
    <em:targetApplication>
      <Description>
        <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
        <em:minVersion>2.0</em:minVersion>
        <em:maxVersion>2.7.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>QuoteCollapse</em:name>
    <em:description>Collapse quotes.</em:description>
    <em:creator>Michael J Gruber</em:creator>
    <em:homepageURL>http://quotecollapse.mozdev.org/</em:homepageURL>
    <em:iconURL>chrome://quotecollapse/content/quotecollapse.png</em:iconURL>
    <!-- Front End Integration Hooks (used by Extension Manager)-->
    <!-- 
    <em:optionsURL>chrome://quotecollapse/content/options.xul</em:optionsURL>
    <em:aboutURL>chrome://quotecollapse/content/options.xul</em:aboutURL>
    -->
    
    <!-- Packages, Skins and Locales that this extension registers -->
    <em:file>
      <Description about="urn:mozilla:extension:file:quotecollapse.jar">
        <em:package>content/quotecollapse/</em:package>
<!--        <em:locale>locale/en-US/quotecollapse/</em:locale> -->
        <em:skin>skin/classic/quotecollapse/</em:skin> 
        <em:skin>skin/mcrystal/quotecollapse/</em:skin>
      </Description>
    </em:file>
  </Description>      
</RDF>
